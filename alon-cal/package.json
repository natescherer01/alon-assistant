{
  "name": "alon-cal",
  "version": "1.0.0",
  "description": "Calendar Integration Platform - Full-stack application with Vite + React frontend and Express + Prisma backend",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build": "npm run build:frontend && npm run build:backend",
    "db:start": "docker compose up -d postgres",
    "db:stop": "docker compose down",
    "db:studio": "cd backend && npm run prisma:studio",
    "db:migrate": "cd backend && npm run prisma:migrate",
    "db:generate": "cd backend && npm run prisma:generate"
  },
  "keywords": [
    "calendar",
    "integration",
    "vite",
    "react",
    "express",
    "prisma",
    "postgresql",
    "oauth"
  ],
  "author": "",
  "license": "ISC"
}
